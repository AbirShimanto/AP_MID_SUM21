
@{
    ViewBag.Title = "Department";
}

<h2>Department</h2>

@model List<LabTask.Models.Department>


<div class="container">
    <table class="table table-condensed">
        <tr>
            <th>ID</th>
            <th>NAME</th>
          
            <th></th>
            <th></th>
        </tr>
        @foreach (var Department in @Model)
        {
            <tr>
                <td>@Department.Id</td>
                <td>@Department.Name</td>
                

                <td><a class="btn btn-success" href="/Student/Index">Go Back</a></td>
                
            </tr>
        }

    </table>
</div>

